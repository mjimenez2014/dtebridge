= simple_form_for(@comisione) do |f|
  = f.error_notification

  .form-inputs
    = f.input :NroLinCom
    = f.input :integer
    = f.input :TipoMovim
    = f.input :string
    = f.input :Glosa
    = f.input :string
    = f.input :ValComNeto
    = f.input :integer
    = f.input :ValComExe
    = f.input :integer
    = f.input :ValComIVA
    = f.input :integer
    = f.association :documento

  .form-actions
    = f.button :submit
