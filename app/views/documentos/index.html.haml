%h1 Documentos

= will_paginate @documentos

= form_tag '/buscarDoc' do
  = field_set_tag do
    = label_tag :empresa, 'Empresa:'
    %input{name: 'empresa', type: 'text', list: 'Empresa'}
    %datalist#Empresa
      %option Lubba Santiago 
      %option Lubba Regiones
    = label_tag :sucursal, 'Sucursal:'
    %input{name: 'sucursal', type: 'text', list: 'Sucursal'}
    %datalist#Sucursal
      %option (Regiones) ARLEGUI 951, VINA DEL MAR  
      %option (Regiones) INDEPENDENCIA 336, OVALLE  
      %option (Regiones) ARTURO PRAT 665, LA SERENA  
      %option (Regiones) DOMINGO CORREA 25, LA CISTERNA  
      %option (Regiones) AV. JOSE MIGUEL CARRERA 8158, LA CISTERNA
      %option (Stgo) MONUMENTO 1963, MAIPU
      %option (Stgo) AV JOSE MIGUEL CARRERA 8158, LA CISTERNA
      %option (Stgo) DOMINGO CORREA 25, LA CISTERNA
      %option (Stgo) 5 DE ABRIL 412-A,  MAIPU

    = label_tag :Folio, 'Folio:'
    = text_field_tag :Folio
    = submit_tag 'Buscar'
